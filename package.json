{
  "name": "graphql-client-benchmarks",
  "description": "A benchmarking suite for GraphQL clients, with focus on their cache implementations",
  "license": "Apache-2.0",
  "version": "1.1.0",
  "repository": "convoyinc/graphql-client-benchmarks",
  "homepage": "https://github.com/convoyinc/graphql-client-benchmarks",
  "bugs": "https://github.com/convoyinc/graphql-client-benchmarks/issues",
  "scripts": {
    "format": "prettier . -w --cache --experimental-cli",
    "cli": "node --expose-gc ./src/reporters/cli/entry.js",
    "compile": "yarn compile-ts && yarn compile-web",
    "compile-artifacts": "./scripts/compile-artifacts.sh",
    "compile-ts": "tsc",
    "check": "tsc --noEmit",
    "compile-web": "rspack --config ./src/reporters/web/webpack.config.js --mode production",
    "dev": "rspack serve --config ./src/reporters/web/webpack.config.js",
    "update-pages": "./scripts/update-pages.sh",
    "release": "./scripts/release.sh",
    "lint": "oxlint --fix",
    "relay": "relay-compiler",
    "isograph": "iso"
  },
  "bin": "./src/reporters/cli/entry.js",
  "main": "./src",
  "dependencies": {
    "@apollo/client": "^3.8.7",
    "@convoy/dapper": "^2.0.104",
    "@isograph/compiler": "main",
    "@isograph/react": "main",
    "@types/bluebird": "^3.5.36",
    "babel-plugin-relay": "^20.0.0",
    "bluebird": "^3.7.2",
    "browser-process-hrtime": "^1.0.0",
    "chroma-js": "^3.0.0",
    "fast-random": "^2.0.4",
    "fast-stats": "^0.0.7",
    "graphql": "^16.5.0",
    "graphql-tag": "^2.12.6",
    "ink": "^0.5.0",
    "le-table": "^7.0.0",
    "lodash": "^4.17.10",
    "path-browserify": "^1.0.1",
    "performance-now": "^2.1.0",
    "process": "^0.11.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "relay-compiler": "^20.0.0",
    "relay-runtime": "^20.0.0",
    "require-context": "https://github.com/bojidar-bg/require-context/archive/1e779871a2cccd9bd1da85a43482365c1fb964a2.tar.gz",
    "shelljs": "^0.10.0",
    "source-map-support": "^0.5.6",
    "tslib": "~2.8.0",
    "typescript": "~4.9.0",
    "unexpected": "^13.0.0",
    "uuid": "^13.0.0",
    "write-file-p": "^1.0.8"
  },
  "devDependencies": {
    "@prettier/plugin-oxc": "^0.0.4",
    "@rspack/cli": "^1.6.3",
    "@rspack/core": "^1.6.3",
    "@types/chroma-js": "^3.0.0",
    "@types/fast-stats": "^0.0.35",
    "@types/lodash": "^4.14.110",
    "@types/node": "^24.0.0",
    "@types/react": "^18.0.14",
    "@types/relay-compiler": "^14.0.3",
    "@types/relay-runtime": "^19.0.0",
    "@types/webpack-env": "^1.17.0",
    "oxlint": "^1.28.0",
    "prettier": "latest"
  },
  "engines": {
    "node": "24.11.1"
  },
  "packageManager": "yarn@3.8.7"
}
