{
	"compilerOptions": {
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"jsx": "react",
		"strictFunctionTypes": true,
		"lib": [
			"es2016",
			"es2017.intl",
			"es2017.object",
			"es2017.string",
			"es2017.typedarrays",
			"dom",
			"esnext.asynciterable"
		],
		"alwaysStrict": true,
		"skipLibCheck": true,
		"module": "CommonJS",
		"target": "es2017"
	}
}
